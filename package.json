{
  "name": "@etchteam/storybook-addon-marker",
  "version": "2.0.3",
  "description": "Add a Marker.io feedback button to the storybook UI",
  "main": "dist/preset.js",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "prepare": "husky install; babel ./src --out-dir ./dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etchteam/storybook-addon-marker.git"
  },
  "keywords": [
    "storybook",
    "addon",
    "storybook-addon",
    "marker",
    "feedback"
  ],
  "author": "etchteam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/etchteam/storybook-addon-marker/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Marker.io",
    "icon": "https://raw.githubusercontent.com/etchteam/storybook-addon-marker/master/marker-storybook-logo.png"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@etchteam/eslint-config": "^1.7.1",
    "@etchteam/stylelint-config": "^1.5.0",
    "@storybook/addon-actions": "^7.0.9",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/react-webpack5": "^7.0.9",
    "babel-loader": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "storybook": "^7.0.9",
    "stylelint": "^15.11.0"
  },
  "dependencies": {
    "@marker.io/browser": "^0.10.0",
    "@storybook/api": "^7.0.9",
    "@storybook/components": "^7.0.9",
    "@storybook/manager-api": "^7.0.9",
